<view class='page'>
  <navigator class="msgbox">
    <view class="msgbox_in">
      <view class="msgbox_in_img">
        <open-data type="userAvatarUrl" class="msgbox_in_img" id="userinfo"></open-data>
      </view>
      <view class="msgbox_in_text_title">{{man.studentName}}</view>
      <view class="msgbox_in_text_desc">孙雨晶</view>
      <view class="msgbox_in_text_desc">软件工程</view>
    </view>
  </navigator>

  <view class="page__bd">
    <!--view class="weui-cells__title">新消息提示跟摘要信息后，统一在列表右侧</view>
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell weui-cell_access">
        <view class="weui-cell__bd">单行列表</view>
        <view class="weui-cell__ft weui-cell__ft_in-access" style="font-size: 0">
          <view style="display: inline-block;vertical-align:middle; font-size: 17px;">详细信息</view>
          <view class="weui-badge weui-badge_dot" style="margin-left: 5px;margin-right: 5px;"></view>
        </view>
      </view>
    </view-->

    <view class="weui-cells__title">个人基本信息</view>
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell weui-cell_access">
        <view class="weui-cell__bd">
          <view style="display: inline-block; vertical-align: middle">基本信息</view>
        </view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </view>
      <view class="weui-cell weui-cell_access">
        <view class="weui-cell__bd">
          <view style="display: inline-block; vertical-align: middle">个人标签</view>
        </view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </view>
      <view class="weui-cell weui-cell_access">
        <view class="weui-cell__bd">
          <view style="display: inline-block; vertical-align: middle">最近提醒</view>
          <view class="weui-badge" style="margin-left: 5px;">8</view>
          <!--view class="weui-badge" style="margin-left: 5px;">New</view-->
        </view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </view>
    </view>
  </view>

  <view class="page__bd">
    <view class="weui-cells__title">招聘信息</view>
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell weui-cell_access">
        <view class="weui-cell__bd">
          <view style="display: inline-block; vertical-align: middle">我创建的招聘</view>
          <view class="weui-badge" style="margin-left: 5px;">New</view>
        </view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </view>
      <view class="weui-cell weui-cell_access">
        <view class="weui-cell__bd">
          <view style="display: inline-block; vertical-align: middle">我关注的招聘</view>
          <view class="weui-badge" style="margin-left: 5px;">New</view>
        </view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </view>
      <view class="weui-cell weui-cell_access">
        <view class="weui-cell__bd">
          <view style="display: inline-block; vertical-align: middle">我应聘的招聘</view>
          <view class="weui-badge" style="margin-left: 5px;">New</view>
          <!--view class="weui-badge" style="margin-left: 5px;">New</view-->
        </view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </view>
    </view>
  </view>
</view>